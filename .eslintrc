{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "semi": ["error", "never"],
    "comma-dangle": ["error", "never"],
    "no-param-reassign": ["off"],
    "import/no-unresolved": ["off"]
  },
  "globals": {
    "Phraser": true
  }
}
